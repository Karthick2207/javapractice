class Solution {
    public int singleNonDuplicate(int[] nums) {
        int[] arr = new int[1000000];
        for (int i = 0; i < nums.length; i++) {
            arr[nums[i]]++;
        }
        int value = 0;
        for (int i = 1; i < arr.length; i++) {
            if (arr[i]==1) {
                    value = i;
                }
            }
        return value;
    }
}
