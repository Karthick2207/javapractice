class Solution {
    public boolean isPalindrome(int x) {
        if(x<0){
            return false;
        }
        else{
            int original=x;
            int reverse=0;
            while(x!=0){
                int h=x%10;
                reverse=reverse*10+h;
                x=x/10;
            }
            return original==reverse;
        }
        
    }
}
