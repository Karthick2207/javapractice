class Solution {
    public int sumOfUnique(int[] nums) {
        HashSet<Integer> unique = new HashSet<>();
        HashSet<Integer> duplicate = new HashSet<>();

        for (int num : nums) {
            if (duplicate.contains(num)) {
                continue;
            }
            if (unique.contains(num)) {
                unique.remove(num);
                duplicate.add(num);
            } else {
                unique.add(num);
            }
        }

        int total = 0;
        for (int num : unique) {
            total += num;
        }

        return total;
    }
}
